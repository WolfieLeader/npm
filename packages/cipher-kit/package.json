{
  "name": "cipher-kit",
  "version": "2.1.1",
  "description": "ðŸ” Secure, Modern, and Cross-Platform Cryptography Helpers for Web, Node.js, Deno, Bun, and Cloudflare Workers",
  "homepage": "https://github.com/WolfieLeader/npm/tree/main/packages/cipher-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WolfieLeader/npm.git",
    "directory": "packages/cipher-kit"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./web-api": {
      "import": {
        "types": "./dist/web-api.d.ts",
        "default": "./dist/web-api.js"
      },
      "require": {
        "types": "./dist/web-api.d.cts",
        "default": "./dist/web-api.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@internal/helpers": "workspace:*",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "WolfieLeader",
  "license": "MIT",
  "keywords": [
    "encrypt",
    "crypto",
    "cipher",
    "security",
    "hash",
    "password",
    "uuid",
    "web",
    "node",
    "deno",
    "bun",
    "cloudflare"
  ]
}
